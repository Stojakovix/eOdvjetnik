<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="eOdvjetnik.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:eOdvjetnik"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Početna"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    

    <ShellContent
        Title="Kalendar"
        ContentTemplate="{DataTemplate local:Kalendar}"
        Route="Kalendar"
        />

    <ShellContent
        Title="Dokumenti"
        ContentTemplate="{DataTemplate local:Dokumenti}"
        Route="Dokumenti"
        />
</Shell>
